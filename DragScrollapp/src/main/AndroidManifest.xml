<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.snail.labaffinity">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">

        </activity>
        <activity android:name=".activity.FragmentTabHostSlidingLayoutActivity" />
        <activity android:name=".activity.ViewPagerlidingLayoutActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.DragListViewActivity">

        </activity>
        <activity android:name=".activity.DragWebViewActivity" />
        <activity android:name=".activity.DragWebViewListActivity" />

        <service android:name=".service.BackGroundService" />
    </application>
</manifest>